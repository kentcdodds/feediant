{
  "name": "feediant",
  "private": true,
  "type": "module",
  "imports": {
    "#app/*": "./app/*",
    "#tests/*": "./tests/*"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "inspect": "mcp-inspector --config mcp-inspector-config.json --server feediant",
    "build": "react-router build",
    "dev": "react-router dev",
    "setup": "cp .env.example .env && npm install && npm run validate",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "validate": "npm run typecheck && npm run lint && npm run test"
  },
  "dependencies": {
    "@epic-web/cachified": "^5.6.0",
    "@epic-web/invariant": "^1.0.0",
    "@epic-web/remember": "^1.1.0",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@prisma/adapter-better-sqlite3": "^6.12.0",
    "@prisma/client": "^6.12.0",
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "@remix-run/lazy-file": "^3.5.0",
    "isbot": "^5.1.27",
    "lru-cache": "^11.1.0",
    "match-sorter": "^8.1.0",
    "md5-hex": "^5.0.0",
    "mime-types": "^3.0.1",
    "music-metadata": "^11.7.3",
    "p-limit": "^6.2.0",
    "prisma": "^6.12.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@epic-web/config": "^1.21.1",
    "@faker-js/faker": "^9.9.0",
    "@modelcontextprotocol/inspector": "^0.16.2",
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.4",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.32.0",
    "get-port": "^7.1.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "prettier": "@epic-web/config/prettier"
}
